# Copy to .env and fill in your credentials
RWGPS_API_KEY=your_api_key_here
RWGPS_EMAIL=you@example.com
RWGPS_PASSWORD=your_password_here
# Optional: re-use a saved auth token instead of email/password login each run
RWGPS_AUTH_TOKEN=

# Control behavior
RWGPS_VERSION=2
RWGPS_DRY_RUN=false
RWGPS_BASE_URL=https://ridewithgps.com